        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="165dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="452dp"
        android:layout_height="724dp"
        android:scaleType="center"
